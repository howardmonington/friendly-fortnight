    "import math\n",
    "\n",
    "def summation_with_func(low, high, func):\n",
    "    sum = 0\n",
    "    for i in range(low, high + 1):\n",
    "        sum += func(i)\n",
    "    return sum\n",
    "\n",
    "def pois_pdf(x, lam = 120):\n",
    "    return ((lam**x*math.exp(-lam))/(math.factorial(x)))\n",
    "\n",
    "print(1 - summation_with_func(0, 99, pois_pdf))"
